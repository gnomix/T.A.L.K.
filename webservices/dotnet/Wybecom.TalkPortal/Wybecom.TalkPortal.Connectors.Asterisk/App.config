            </setting>
            <setting name="FeatureCodeCallForwardAllDeactivate" serializeAs="String">
                <value>*73</value>
            </setting>
            <setting name="FeatureCodeDNDToggle" serializeAs="String">
                <value>*76</value>
            </setting>
            <setting name="FeatureCodeDNDActivate" serializeAs="String">
                <value>*78</value>
            </setting>
            <setting name="FeatureCodeDNDDeactivate" serializeAs="String">
